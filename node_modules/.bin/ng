#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/projects/services/node_modules/.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules/@angular/cli/bin/node_modules:/mnt/d/projects/services/node_modules/.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules/@angular/cli/node_modules:/mnt/d/projects/services/node_modules/.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules/@angular/node_modules:/mnt/d/projects/services/node_modules/.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules:/mnt/d/projects/services/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/projects/services/node_modules/.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules/@angular/cli/bin/node_modules:/mnt/d/projects/services/node_modules/.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules/@angular/cli/node_modules:/mnt/d/projects/services/node_modules/.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules/@angular/node_modules:/mnt/d/projects/services/node_modules/.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules:/mnt/d/projects/services/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules/@angular/cli/bin/ng.js" "$@"
else
  exec node  "$basedir/../.pnpm/@angular+cli@19.1.6_@types+node@22.13.1/node_modules/@angular/cli/bin/ng.js" "$@"
fi
